{
  "name": "carcer-special-events",
  "displayName": "Carcer Special Events",
  "description": "Syntax highlighting and language support for Carcer special event files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "carcer-special-events",
        "aliases": [
          "Carcer Special Events",
          "se"
        ],
        "extensions": [
          ".se"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "carcer-special-events",
        "scopeName": "source.carcerSpecialEvents",
        "path": "./syntaxes/carcer-special-events.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "carcer-special-events",
        "path": "./snippets/events.json"
      }
    ]
  }
}
